    <modelVersion>4.0.0</modelVersion>

    <artifactId>provider</artifactId>

    <dependencies>
        <dependency>
            <groupId>com.tuling</groupId>
            <artifactId>interface</artifactId>
            <version>1.0-SNAPSHOT</version>
